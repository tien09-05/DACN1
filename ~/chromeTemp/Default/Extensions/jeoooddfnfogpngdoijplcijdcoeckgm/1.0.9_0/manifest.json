{
   "author": "eyeo GmbH",
   "background": {
      "persistent": true,
      "scripts": [ "polyfill.js", "ext/common.js", "ext/background.js", "lib/adblockplus.js" ]
   },
   "browser_action": {
      "default_icon": {
         "16": "icons/iconBlockAd-16.png",
         "32": "icons/iconBlockAd-32.png"
      },
      "default_popup": "popup.html",
      "default_title": "__MSG_name__"
   },
   "content_scripts": [ {
      "all_frames": true,
      "js": [ "polyfill.js", "ext/common.js", "ext/content.js", "include.preload.js" ],
      "match_about_blank": true,
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   }, {
      "all_frames": true,
      "js": [ "include.postload.js" ],
      "match_about_blank": true,
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_end"
   } ],
   "default_locale": "en_US",
   "description": "__MSG_description__",
   "devtools_page": "devtools.html",
   "icons": {
      "16": "icons/iconBlockAd-16.png",
      "32": "icons/iconBlockAd-32.png",
      "48": "icons/detailed/iconBlockAd-48.png",
      "64": "icons/detailed/iconBlockAd-64.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "__MSG_name_releasebuild__",
   "optional_permissions": [ "contentSettings", "management" ],
   "options_ui": {
      "open_in_tab": true,
      "page": "options.html"
   },
   "permissions": [ "tabs", "\u003Call_urls>", "webRequest", "webRequestBlocking", "webNavigation", "storage", "unlimitedStorage", "notifications", "metricsPrivate" ],
   "short_name": "__MSG_name__",
   "storage": {
      "managed_schema": "managed-storage-schema.json"
   },
   "update_url": "https://browser.coccoc.com/service/update2/crx",
   "version": "1.0.9"
}
